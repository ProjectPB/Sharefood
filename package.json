    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "algoliasearch": "^4.13.0",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "firebase": "^9.6.8",
    "nth-check": "^2.1.1",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-cropper": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-draft-wysiwyg": "^1.14.7",
    "react-helmet-async": "^1.3.0",
    "react-image-file-resizer": "^0.4.7",
    "react-instantsearch-dom": "^6.23.3",
    "react-moment": "^1.1.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.1",
    "react-select": "^5.3.2",
    "react-share": "^4.4.0",
    "redux": "^4.0.5",
    "redux-deep-persist": "^1.0.6",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.4.0",
    "sass": "^1.52.0",
    "start": "^5.1.0",
    "typescript": "^4.5.5",
    "update-notifier": "^6.0.2",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run delete-maps",
    "postbuild": "react-snap",
    "delete-maps": "npm run delete-map-files && npm run delete-references-to-map-files",
    "delete-map-files": "find ./build -name '*.map' -delete",
    "delete-references-to-map-files": "find ./build -regex '.*\\.\\(js\\|css\\)' -exec sed -i -E '\\/[\\*\\/]#\\ssourceMappingURL=main(\\.[0-9a-f]+)?\\.(css|js)\\.map(\\*\\/)?/g' {} +",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "reactSnap": {
    "inlineCss": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/draft-js": "^0.11.9",
    "@types/draftjs-to-html": "^0.8.1",
    "@types/react-copy-to-clipboard": "^5.0.3",
    "@types/react-draft-wysiwyg": "^1.13.4",
    "@types/react-instantsearch-dom": "^6.12.2",
    "@types/react-redux": "^7.1.22",
    "react-snap": "^0.2.0"
  }
}
